[PrefixDeclaration]
:       http://example.org/resource#
schema: http://schema.org/
xsd:    http://www.w3.org/2001/XMLSchema#

[MappingDeclaration] @collection [[
mappingId       UserMapping
target          :User/{tax_id} a schema:Person ; schema:identifier {tax_id} ; schema:name {name} ; schema:email {email} ; schema:telephone {phone_number} ; schema:address {address} .
source          SELECT tax_id, name, email, phone_number, address FROM user_profiles
]]